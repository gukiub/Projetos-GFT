@{
    ViewBag.Title = "Projetos";
}
<h2>@ViewBag.Title.</h2>
<hr />
<p>Aqui temos prints de alguns de meus projetos.</p>

<div id="myCarousel" class="carousel slide" data-ride="carousel">
    <!-- Indicators -->
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
        <li data-target="#myCarousel" data-slide-to="4"></li>
        <li data-target="#myCarousel" data-slide-to="5"></li>
        <li data-target="#myCarousel" data-slide-to="6"></li>
        <li data-target="#myCarousel" data-slide-to="7"></li>
        <li data-target="#myCarousel" data-slide-to="8"></li>
    </ol>

    <!-- Wrapper for slides -->
    <div class="carousel-inner">
        <div class="item active">
            <img src="~/Content/Img/1.png" alt="Los Angeles" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/2.png" alt="Chicago" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/3.png" alt="Chicago" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/4.png" alt="New York" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/5.png" alt="New York" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/6.png" alt="New York" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/7.png" alt="New York" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/8.png" alt="New York" width="1366px" height="768px">
        </div>
        <div class="item">
            <img src="~/Content/Img/9.png" alt="New York" width="1366px" height="768px">
        </div>
    </div>

    <!-- Left and right controls -->
    <a class="left carousel-control" href="#myCarousel" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="right carousel-control" href="#myCarousel" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
